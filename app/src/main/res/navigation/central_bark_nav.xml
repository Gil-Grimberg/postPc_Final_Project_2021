<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/central_bark_nav"
    app:startDestination="@id/openingFragment">

    <fragment
        android:id="@+id/openingFragment"
        android:name="com.example.centralbark_PostPc_2021.openingFragment"
        android:label="fragment_opening"
        tools:layout="@layout/fragment_opening" >
        <action
            android:id="@+id/action_openingFragment_to_signInFragment"
            app:destination="@id/signInFragment" />
        <action
            android:id="@+id/action_openingFragment_to_signUpFragment"
            app:destination="@id/signUpFragment" />
    </fragment>
    <fragment
        android:id="@+id/signInFragment"
        android:name="com.example.centralbark_PostPc_2021.signInFragment"
        android:label="fragment_sign_in"
        tools:layout="@layout/fragment_sign_in" >
        <action
            android:id="@+id/action_signInFragment_to_feedFragment"
            app:destination="@id/feedFragment" />
    </fragment>
    <fragment
        android:id="@+id/signUpFragment"
        android:name="com.example.centralbark_PostPc_2021.signUpFragment"
        android:label="fragment_sign_up"
        tools:layout="@layout/fragment_sign_up" >
        <action
            android:id="@+id/action_signUpFragment_to_matchAFriendFragment"
            app:destination="@id/matchAFriendFragment" />
    </fragment>
    <fragment
        android:id="@+id/feedFragment"
        android:name="com.example.centralbark_PostPc_2021.FeedFragment"
        android:label="fragment_feed"
        tools:layout="@layout/fragment_feed" >
        <action
            android:id="@+id/action_feedFragment_to_notificationsFragment"
            app:destination="@id/notificationsFragment" />
    </fragment>
    <fragment
        android:id="@+id/notificationsFragment"
        android:name="com.example.centralbark_PostPc_2021.notificationsFragment"
        android:label="fragment_notifications"
        tools:layout="@layout/fragment_notifications" />
    <fragment
        android:id="@+id/matchAFriendFragment"
        android:name="com.example.centralbark_PostPc_2021.matchAFriendFragment"
        android:label="fragment_match_a_friend"
        tools:layout="@layout/fragment_match_a_friend" />
    <fragment
        android:id="@+id/myProfileFragment"
        android:name="com.example.centralbark_PostPc_2021.myProfileFragment"
        android:label="fragment_my_profile"
        tools:layout="@layout/fragment_my_profile" >
        <action
            android:id="@+id/action_myProfileFragment_to_settingsFragment"
            app:destination="@id/settingsFragment" />
    </fragment>
    <fragment
        android:id="@+id/otherProfileFragment"
        android:name="com.example.centralbark_PostPc_2021.otherProfileFragment"
        android:label="fragment_other_profile"
        tools:layout="@layout/fragment_other_profile" >
        <action
            android:id="@+id/action_otherProfileFragment_to_searchAcountFragment"
            app:destination="@id/searchAcountFragment" />
    </fragment>
    <fragment
        android:id="@+id/searchAcountFragment"
        android:name="com.example.centralbark_PostPc_2021.searchAccountFragment"
        android:label="fragment_search_acount"
        tools:layout="@layout/fragment_search_account" >
        <action
            android:id="@+id/action_searchAcountFragment_to_otherProfileFragment"
            app:destination="@id/otherProfileFragment" />
        <action
            android:id="@+id/action_searchAcountFragment_to_searchPlacesFragment"
            app:destination="@id/searchPlacesFragment" />
    </fragment>
    <fragment
        android:id="@+id/searchPlacesFragment"
        android:name="com.example.centralbark_PostPc_2021.searchPlacesFragment"
        android:label="fragment_search_places"
        tools:layout="@layout/fragment_search_places" >
        <action
            android:id="@+id/action_searchPlacesFragment_to_mapsFragment"
            app:destination="@id/mapsFragment" />
        <action
            android:id="@+id/action_searchPlacesFragment_to_searchAcountFragment"
            app:destination="@id/searchAcountFragment" />
    </fragment>
    <fragment
        android:id="@+id/mapsFragment"
        android:name="com.example.centralbark_PostPc_2021.MapsFragment"
        android:label="fragment_maps"
        tools:layout="@layout/fragment_maps" />
    <fragment
        android:id="@+id/settingsFragment"
        android:name="com.example.centralbark_PostPc_2021.settingsFragment"
        android:label="fragment_settings"
        tools:layout="@layout/fragment_settings" >
        <action
            android:id="@+id/action_settingsFragment_to_acountSettingFragment"
            app:destination="@id/acountSettingFragment" />
        <action
            android:id="@+id/action_settingsFragment_to_privacySettingsFragment"
            app:destination="@id/privacySettingsFragment" />
    </fragment>
    <fragment
        android:id="@+id/acountSettingFragment"
        android:name="com.example.centralbark_PostPc_2021.accountSettingFragment"
        android:label="fragment_acount_setting"
        tools:layout="@layout/fragment_acount_setting" />
    <fragment
        android:id="@+id/privacySettingsFragment"
        android:name="com.example.centralbark_PostPc_2021.privacySettingsFragment"
        android:label="fragment_privacy_settings"
        tools:layout="@layout/fragment_privacy_settings" />
    <fragment
        android:id="@+id/menuFragment"
        android:name="com.example.centralbark_PostPc_2021.MenuFragment"
        android:label="fragment_menu"
        tools:layout="@layout/fragment_menu" >
        <action
            android:id="@+id/action_menuFragment_to_searchAcountFragment"
            app:destination="@id/searchAcountFragment" />
        <action
            android:id="@+id/action_menuFragment_to_feedFragment2"
            app:destination="@id/feedFragment" />
        <action
            android:id="@+id/action_menuFragment_to_matchAFriendFragment"
            app:destination="@id/matchAFriendFragment" />
        <action
            android:id="@+id/action_menuFragment_to_mapsFragment"
            app:destination="@id/mapsFragment" />
        <action
            android:id="@+id/action_menuFragment_to_myProfileFragment"
            app:destination="@id/myProfileFragment" />
    </fragment>
</navigation>