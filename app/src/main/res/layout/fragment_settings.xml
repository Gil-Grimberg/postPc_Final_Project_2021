<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout5"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".FeedFragment">


    <TextView
        android:id="@+id/title_setting_screen"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="25dp"
        android:layout_marginTop="25dp"
        android:fontFamily="casual"
        android:text="@string/settings"
        android:textSize="28sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/privacy_logo_setting_screen"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:src="@drawable/privacy_setting_icon"
        android:layout_marginTop="18dp"
        android:layout_marginStart="40dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/title_setting_screen" />

    <TextView
        android:id="@+id/privacy_option_title_setting_screen"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="25dp"
        android:layout_marginStart="10dp"
        android:text="@string/privacy"
        android:fontFamily="casual"
        app:layout_constraintStart_toEndOf="@+id/privacy_logo_setting_screen"
        app:layout_constraintTop_toBottomOf="@+id/title_setting_screen" />

    <ImageView
        android:id="@+id/enter_to_privacy_button_setting_screen"
        android:layout_width="120dp"
        android:layout_height="35dp"
        android:layout_marginTop="20dp"
        android:src="@drawable/enter_to_icon"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/title_setting_screen" />

    <ImageView
        android:id="@+id/account_logo_setting_screen"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_marginTop="18dp"
        android:layout_marginStart="40dp"
        android:src="@drawable/account_setting_icon"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/privacy_logo_setting_screen" />
</androidx.constraintlayout.widget.ConstraintLayout>